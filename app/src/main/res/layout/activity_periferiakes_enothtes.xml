<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".periferiakes_enothtes"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="-41dp">

    <include
        android:id="@+id/include"
        layout="@layout/toolbar" />

    <Button
        android:id="@+id/an_makedonia"
        android:layout_marginStart="10dp"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="ΑΝ. ΜΑΚΕΔΟΝΙΑ-ΘΡΑΚΗ"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"

        android:layout_centerHorizontal="true"
        android:layout_below="@id/include"/>
    <Button
        android:id="@+id/button2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="ΚΕΝΤΡΙΚΗ-ΜΑΚΕΔΟΝΙΑ"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"

        android:layout_below="@id/an_makedonia"/>
    <Button
        android:id="@+id/button3"
        android:layout_width="300dp"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:text="ΔΥΤΙΚΗ-ΜΑΚΕΔΟΝΙΑ"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"
        android:layout_below="@id/button2"/>

    <Button
        android:id="@+id/button4"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button3"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"

        android:text="ΗΠΕΙΡΟΣ" />

    <Button
        android:id="@+id/button5"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button4"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΘΕΣΣΑΛΙΑ" />

    <Button
        android:id="@+id/button6"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button5"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΔΥΤΙΚΗ ΕΛΛΑΔΑ" />

    <Button
        android:id="@+id/button7"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button6"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΣΤΕΡΕΑ ΕΛΛΑΔΑ" />

    <Button
        android:id="@+id/button8"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button7"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΑΤΤΙΚΗ" />
    <Button
        android:id="@+id/button9"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button8"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΠΕΛΛΟΠΟΝΝΗΣΟΣ" />
    <Button
        android:id="@+id/button10"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button9"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΙΟΝΙΑ ΝΗΣΙΑ" />
    <Button
        android:id="@+id/button11"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button10"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΒΟΡΕΙΟ ΑΙΓΑΙΟ" />
    <Button
        android:id="@+id/button12"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button11"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΝΟΤΙΟ ΑΙΓΑΙΟ" />
    <Button
        android:id="@+id/button13"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/button12"
        android:layout_marginStart="10dp"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="10dp"
        android:background="@drawable/rounded_corners"


        android:text="ΚΡΗΤΗ" />




</RelativeLayout>